<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.SpLTenth.mybatis.IUserIDUQ">

    <select id="getUser" parameterType="int"
        resultType="org.SpLTenth.mybatis.UserIDUQ">
        SELECT *
        FROM mybatisuser
        WHERE id = #{userId}
    </select>


    <insert id="insertUser" parameterType="UserIDUQ">
        INSERT INTO mybatisuser(id,
        username, mobile,created)
        VALUES(#{id}, #{name}, #{mobile}, #{created})
    </insert>

    <select id="getUserIDUQList" resultType="org.SpLTenth.mybatis.UserIDUQ">
        SELECT * FROM mybatisuser
    </select>

    <update id="updateUser" parameterType="UserIDUQ">
        UPDATE mybatisuser
        SET
        username=
        #{name},
        mobile = #{mobile},
        created = #{created}
        WHERE
        id =
        #{id}
    </update>

    <delete id="deleteUser" parameterType="int">
        DELETE FROM mybatisuser WHERE id = #{id}
    </delete>

</mapper>